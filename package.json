{
  "name": "@libra-foundation/eslint-config",
  "version": "0.0.0",
  "description": "Eslint barebone configuration. Not speciffically tailored for either JS or TS.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Libra-foundation/eslint-configs.git"
  },
